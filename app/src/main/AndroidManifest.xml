<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uschedule.scheduler">

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/web_hi_res_512"
        android:label="@string/app_name"
        android:roundIcon="@drawable/web_hi_res_512"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.uschedule.scheduler.finalUI.CompareSchedules" />
        <activity android:name="com.uschedule.scheduler.mainActivities.ThirdActivity">
        </activity>
        <activity
            android:name="com.uschedule.scheduler.mainActivities.MainActivity"
            android:label=""
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.uschedule.scheduler.social.searchBar" />


    </application>

</manifest>